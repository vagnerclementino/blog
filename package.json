{
  "name": "clementino-notes",
  "engines": {
    "node": ">=20.14.0",
    "npm": ">=10.7.0"
  },
  "private": true,
  "description": "A personal blog, based on the default blog starter",
  "version": "3.0.0",
  "author": "Vagner Clementino <vagner@clementino.me>",
  "bugs": {
    "url": "https://github.com/vagnerclementino/blog/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.6.22",
    "ajv": "^8.12.0",
    "babel-plugin-styled-components": "^1.13.3",
    "gatsby": "5.12.6",
    "gatsby-cli": "5.12.0",
    "gatsby-plugin-disqus": "^1.2.6",
    "gatsby-plugin-image": "3.12.1",
    "gatsby-plugin-local-search": "^1.1.1",
    "gatsby-plugin-mdx": "^5.14.1",
    "gatsby-plugin-offline": "^6.14.0",
    "gatsby-plugin-released": "^0.3.0",
    "gatsby-plugin-sharp": "5.12.1",
    "gatsby-plugin-styled-components": "^5.25.0",
    "gatsby-plugin-typography": "^5.14.0",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-embed-youtube": "^0.0.7",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^7.0.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-remark-responsive-iframe": "^6.14.0",
    "gatsby-remark-smartypants": "^6.14.0",
    "gatsby-remark-vscode": "^3.3.1",
    "gatsby-source-filesystem": "^5.12.1",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "5.12.1",
    "prismjs": "^1.30.0",
    "query-string": "^6.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^5.2.0",
    "react-switch": "^5.0.0",
    "react-typography": "^0.16.19",
    "react-use-flexsearch": "^0.1.1",
    "reading-time": "^1.5.0",
    "remark-gfm": "^3.0.1",
    "styled-components": "^6.1.0",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@cspell/dict-pt-br": "^2.3.3",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.14.0",
    "cspell": "^8.16.0",
    "gatsby-plugin-root-import": "^2.0.9",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "prettier": "^1.17.0"
  },
  "resolutions": {
    "ajv": "8.12.0"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby",
    "blog",
    "clementino",
    "notes"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:vagnerclementino/blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "build:verbose": "gatsby build --verbose --log-pages",
    "develop": "GATSBY_GTAG_DEBUG=true gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "start:nocache": "npm run clean && npm start",
    "serve": "gatsby serve",
    "debug:ga": "GATSBY_GTAG_DEBUG=true gatsby develop",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "clean": "gatsby clean",
    "prepare": "husky",
    "prepush": "npm run test && cspell link add @cspell/dict-pt-br && cspell --no-must-find-files --config ./cspell.json '**/*.md' && cspell --config ./cspell.json '**/*.mdx'"
  }
}

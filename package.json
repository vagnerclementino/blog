{
  "name": "clementino-notes",
  "private": true,
  "description": "A personal blog, based on the default blog starter",
  "version": "3.0.0",
  "author": "Vagner Clementino <vagner@clementino.me>",
  "bugs": {
    "url": "https://github.com/vagnerclementino/blog/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "babel-plugin-styled-components": "^1.13.3",
    "gatsby": "^4.5.5",
    "gatsby-cli": "^4.25.0",
    "gatsby-plugin-disqus": "^1.2.6",
    "gatsby-plugin-image": "^2.25.0",
    "gatsby-plugin-local-search": "^1.1.1",
    "gatsby-plugin-mdx": "^2.15.0",
    "gatsby-plugin-offline": "^4.15.0",
    "gatsby-plugin-react-helmet": "^4.15.0",
    "gatsby-plugin-released": "^0.3.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-styled-components": "^5.25.0",
    "gatsby-plugin-typography": "^3.15.0",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-footnotes": "0.0.8",
    "gatsby-remark-images": "^3.3.9",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-remark-vscode": "^2.1.2",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-sharp": "^2.12.1",
    "query-string": "^6.13.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-switch": "^5.0.0",
    "react-typography": "^0.16.19",
    "react-use-flexsearch": "^0.1.1",
    "styled-components": "^6.1.0",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "@cspell/dict-pt-br": "^2.3.3",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.14.0",
    "cspell": "^8.16.0",
    "gatsby-plugin-root-import": "^2.0.9",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^1.17.0"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
  "keywords": [
    "gatsby",
    "blog",
    "clementino",
    "notes"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git@github.com:vagnerclementino/blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "build:verbose": "gatsby build --verbose --log-pages",
    "develop": "GATSBY_GTAG_DEBUG=true gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "debug:ga": "GATSBY_GTAG_DEBUG=true gatsby develop",
    "test": "jest",
    "clean": "gatsby clean",
    "prepare": "husky",
    "prepush": "cspell link add @cspell/dict-pt-br && cspell --no-must-find-files --config ./cspell.json '**/*.md' && cspell --config ./cspell.json '**/*.mdx'"
  }
}
